<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @* ----------CSS----------- *@
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />

    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/MyStyles.css" rel="stylesheet" />
    @* ----------JS------------ *@


    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</head>
<body>
    <nav class="navbar navbar-default navbar-fixed-top menu">
        <div class="container">
            <div id="Logo" class="pull-left">
                <a href="Index"><img id="Logo" class="img-responsive" src="~/Content/img/logo1.png" /> </a>
            </div> 


            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

            </div>

            <div class="navbar-collapse collapse navbar-right">
                <ul class="nav navbar-nav">
                    <li class="menupunkter"><a class="menulink" href="">Forside</a></li>
                    <li class="menupunkter"><a class="menulink" href="">Priser</a></li>
                    <li class="menupunkter"><a class="menulink" href="">FAQ</a></li>
                    <li class="menupunkter"><a class="menulink" href="">Log ind</a></li>
                </ul>
            </div>
            <span class="fa fa-2x fa-facebook-official pull-right"></span>
            <span class="fa fa-2x fa-envelope-o pull-right"></span>
        </div>
    </nav>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    
</body>
</html>